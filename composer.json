{
    "name": "sewadar/management-app",
    "description": "Sewadar Management Application using CodeIgniter 4 and Grocery CRUD",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "^4.4",
        "grocerycrud/enterprise": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
